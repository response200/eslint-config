{
  "name": "@response200/eslint-config",
  "version": "1.0.0",
  "description": "Strict ESLint rules for JavaScript/TypeScript/JSX projects in one convenient package",
  "keywords": [
    "eslint",
    "eslintconfig",
    "lint"
  ],
  "main": "default.js",
  "bin": {
    "lint.sh": "scripts/lint.sh"
  },
  "files": [
    "default.js",
    "recommended.js",
    "recommended-jsx.js",
    "recommended-typescript.js",
    "scripts/lint.sh",
    "scripts/helpers/git.sh",
    "CHANGELOG.md",
    "LICENCE.md",
    "README.md",
    "VERSIOHISTORIA.md"
  ],
  "directories": {
    "test": "tests"
  },
  "homepage": "https://github.com/response200/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/response200/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/response200/eslint-config/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Joel Posti",
    "url": "https://response200.pro"
  },
  "funding": {
    "type": "individual",
    "url": "https://paypal.me/joelposti"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=4.14",
    "eslint-config-standard": ">=16",
    "eslint-config-standard-jsx": ">=10",
    "eslint-config-standard-with-typescript": ">=20",
    "eslint-plugin-jsx-a11y": ">=6.4.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.14.5",
    "@cucumber/cucumber": "^7.3",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.1",
    "@types/chai": "^4.2.21",
    "@types/eslint": "^7.28.0",
    "@types/fs-extra": "^9.0.12",
    "@typescript-eslint/parser": "^4.29.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.4",
    "fs-extra": "^10.0.0"
  }
}
