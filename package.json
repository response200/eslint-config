{
  "name": "@response200/eslint-config",
  "version": "2.0.0",
  "description": "Strict ESLint rules for JavaScript/TypeScript/JSX projects in one convenient package",
  "keywords": [
    "eslint",
    "eslintconfig",
    "lint"
  ],
  "main": "default.js",
  "bin": {
    "lint.sh": "scripts/lint.sh"
  },
  "files": [
    "default.js",
    "recommended.js",
    "recommended-jsx.js",
    "recommended-typescript.js",
    "scripts/lint.sh",
    "scripts/helpers/git.sh",
    "CHANGELOG.md",
    "LICENCE.md",
    "README.md",
    "VERSIOHISTORIA.md"
  ],
  "directories": {
    "test": "tests"
  },
  "homepage": "https://github.com/response200/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/response200/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/response200/eslint-config/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Joel Posti",
    "url": "https://response200.pro"
  },
  "funding": {
    "type": "individual",
    "url": "https://paypal.me/joelposti"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-jsx-a11y": "^6.6.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.20.7",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@cucumber/cucumber": "^8.9.1",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@types/chai": "^4.3.4",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.17",
    "@typescript-eslint/parser": "^5.47.0",
    "chai": "^4.3.7",
    "typescript": "^4.9.4"
  }
}
